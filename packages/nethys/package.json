{
	"name": "@kobold/nethys",
	"version": "1.0.0",
	"author": "Significantotter",
	"description": "An interface and parser for nethys data.",
	"license": "MIT",
	"private": true,
	"type": "module",
	"main": "./src/index.js",
	"types": "./src/index.js",
	"exports": {
		"default": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "pnpm run compile",
		"compile": "tsc -p tsconfig.build.json",
		"nethys:import": "node --loader ts-node/esm src/scripts/nethys-import.ts",
		"drizzle:parse-types": "node --loader ts-node/esm src/scripts/parse-nethys-types.ts",
		"drizzle:reset-db": "rm -f ./nethys.db && touch ./nethys.db && drizzle-kit push:sqlite --config=./drizzle.config.ts",
		"drizzle:generate": "drizzle-kit generate:pg",
		"drizzle:migrate": "node --loader ts-node/esm src/scripts/migrate.ts",
		"test": "vitest"
	},
	"dependencies": {
		"@kobold/config": "workspace:*",
		"discord.js": "^14.21.0",
		"drizzle-orm": "^0.33.0",
		"hast-util-from-text": "^3.0.0",
		"hast-util-to-html": "^9.0.5",
		"hast-util-to-string": "^3.0.1",
		"lodash": "^4.17.21",
		"postgres": "^3.4.7",
		"rehype-raw": "^7.0.0",
		"rehype-remark": "^10.0.1",
		"rehype-rewrite": "^4.0.2",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"remark-stringify": "^11.0.0",
		"unified": "^11.0.5",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@elastic/elasticsearch": "^8.19.0",
		"@types/hast": "^3.0.4",
		"@types/lodash": "^4.17.20",
		"@types/mdast": "^4.0.4",
		"@types/node": "^20.19.11",
		"drizzle-kit": "^0.23.2",
		"eslint": "^9.33.0",
		"hast-util-to-mdast": "^10.1.2",
		"node-fetch": "3.3.2",
		"quicktype-core": "^23.2.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.2",
		"vitest": "^0.34.6"
	}
}
