{
	"name": "kobold-client",
	"version": "1.0.0",
	"author": "Significantotter",
	"description": "A discord.js bot for Pathfinder 2e play by post games.",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=20.6.1"
	},
	"main": "dist/start-bot.js",
	"exports": [
		{
			"default": "./dist/start-bot.js",
			"types": "./dist/start-bot.d.ts"
		},
		{
			"default": "./dist/start-manager.js",
			"types": "./dist/start-manager.d.ts"
		}
	],
	"type": "module",
	"scripts": {
		"rebuild": ".\\node_modules\\.bin\\electron-rebuild.cmd -f -m ./node_modules/better-sqlite3",
		"kanel": "ts-node ./src/scripts/kanel-generate.ts",
		"generate-json-schemas": "ts-node ./src/scripts/generate-json-schemas.ts",
		"data-upgrade": "ts-node ./src/scripts/upgrade-sheets-v2.ts",
		"check-circular": "madge --circular --extensions ts ./src",
		"migrate": "ts-node ./src/scripts/migrate.ts",
		"migrate:down": "ts-node ./src/scripts/migrate-down.ts",
		"migrate:test": "NODE_ENV=TEST ts-node ./src/scripts/migrate.ts",
		"lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"clean": "git clean -xdf --exclude=\"/src/config/**/*\"",
		"clean:dry": "git clean -xdf --exclude=\"/src/config/**/*\" --dry-run",
		"build": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run typesafe-i18n && tsc --project tsconfig.json",
		"commands:view": "pnpm run build && node --enable-source-maps dist/start-bot.js commands view",
		"commands:register": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && node --enable-source-maps dist/start-bot.js commands register",
		"commands:rename": "pnpm run build && node --enable-source-maps dist/start-bot.js commands rename",
		"commands:delete": "pnpm run build && node --enable-source-maps dist/start-bot.js commands delete",
		"commands:clear": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && node --enable-source-maps dist/start-bot.js commands clear",
		"dev": "tsc --project tsconfig.json && node --loader=ts-node/esm src/start-bot.ts",
		"start": "pnpm run start:bot",
		"start:bot": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && node --enable-source-maps dist/start-bot.js",
		"start:manager": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && node --enable-source-maps dist/start-manager.js",
		"start:prod": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && pnpm run commands:clear-prod && pnpm run commands:register-prod && pnpm run start:manager-prod",
		"start:pm2": "pnpm run build && pnpm run pm2:start",
		"pm2:start": "pm2 start process.json",
		"pm2:stop": "pm2 stop process.json",
		"pm2:delete": "pm2 delete process.json",
		"typesafe-i18n": "NODE_OPTIONS='--max-old-space-size=4096' typesafe-i18n --no-watch",
		"typesafe-i18n:watch": "typesafe-i18n",
		"knex": "NODE_OPTIONS='--max-old-space-size=4096' NODE_OPTIONS='--loader ts-node/esm' knex",
		"test": "vitest",
		"replace-i18n-types": "cp ./src/i18n/i18n-types.prod.ts ./src/i18n/i18n-types.ts",
		"build-prod": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run replace-i18n-types && tsc --project tsconfig.json",
		"commands:clear-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/start-bot.js commands clear ",
		"commands:register-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/start-bot.js commands register",
		"start:bot-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/start-bot.js",
		"start:manager-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/start-manager.js",
		"reset-pf2etools-db": "rm -f ./compendium.db && touch ./compendium.db && drizzle-kit push:sqlite --config=./drizzle.config.ts",
		"import:pf2etools": "NODE_OPTIONS='--max-old-space-size=4096' pnpm run build && node dist/services/pf2etools/pf2eTools-import.js"
	},
	"dependencies": {
		"@anatine/zod-mock": "^3.13.3",
		"@faker-js/faker": "^8.0.2",
		"axios": "^1.6.7",
		"better-sqlite3": "^8.7.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.0",
		"csv": "^6.3.4",
		"date-fns": "^2.30.0",
		"dice-roller-parser": "^0.1.8",
		"dice-typescript": "^1.6.1",
		"discord.js": "^14.13.0",
		"discord.js-rate-limiter": "1.3.2",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.29.3",
		"drizzle-zod": "^0.5.1",
		"express": "4.18.2",
		"express-promise-router": "4.1.1",
		"filesize": "10.0.12",
		"filtrex": "^3.0.0",
		"form-data": "^4.0.0",
		"json-schema": "^0.4.0",
		"knex": "^2.5.1",
		"kobold-config": "workspace:*",
		"kobold-db": "workspace:*",
		"kysely": "^0.26.3",
		"linguini": "1.3.1",
		"lodash": "^4.17.21",
		"luxon": "3.4.3",
		"node-fetch": "3.3.2",
		"node-schedule": "2.1.1",
		"pf2etools-data": "workspace:*",
		"pg": "^8.11.3",
		"pino": "8.15.1",
		"pino-pretty": "10.2.0",
		"pm2": "^5.3.0",
		"reflect-metadata": "^0.1.13",
		"remove-markdown": "0.5.0",
		"ts-json-schema-generator": "^1.3.0",
		"ts-mixer": "^6.0.3",
		"type-fest": "^4.3.1",
		"typesafe-i18n": "^5.26.2",
		"xxhashjs": "^0.2.2",
		"zod": "^3.22.2",
		"zod-validation-error": "^2.1.0"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/better-sqlite3": "^7.6.5",
		"@types/express": "4.17.17",
		"@types/lodash": "^4.14.202",
		"@types/luxon": "3.3.2",
		"@types/node": "^20.11.19",
		"@types/node-schedule": "2.1.0",
		"@types/pg": "^8.10.2",
		"@types/remove-markdown": "0.3.1",
		"@types/xxhashjs": "^0.2.2",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"drizzle-kit": "^0.20.14",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-unicorn": "^48.0.1",
		"fishery": "^2.2.2",
		"glob": "^10.3.4",
		"json-schema-to-zod": "^1.1.1",
		"kanel": "^3.5.5",
		"kanel-kysely": "^0.2.1",
		"kanel-zod": "^1.2.3",
		"kobold-config": "workspace:*",
		"kobold-db": "workspace:*",
		"madge": "^6.1.0",
		"minimatch": "^9.0.3",
		"pf2etools-data": "workspace:*",
		"prettier": "^3.0.3",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"ts-to-zod": "^3.1.3",
		"typescript": "^5.3",
		"vitest": "^0.34.4",
		"zod-fixture": "^2.5.0",
		"zod-to-json-schema": "^3.22.0",
		"zod-to-ts": "^1.2.0"
	},
	"madge": {
		"tsConfig": "./tsconfig.json",
		"detectiveOptions": {
			"ts": {
				"skipTypeImports": true
			}
		}
	},
	"ts-node": {
		"esm": true,
		"experimentalSpecifierResolution": "node"
	}
}
