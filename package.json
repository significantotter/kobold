{
	"name": "Kobold",
	"version": "1.0.0",
	"author": "Significantotter",
	"description": "A discord.js bot for Pathfinder 2e play by post games.",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=20.6.1"
	},
	"exports": [
		"./dist/start-bot.js",
		"./dist/start-manager.js"
	],
	"type": "module",
	"scripts": {
		"lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
		"check-circular": "madge --debug --circular --extensions ts ./src",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"clean": "git clean -xdf --exclude=\"/src/config/**/*\"",
		"clean:dry": "git clean -xdf --exclude=\"/src/config/**/*\" --dry-run",
		"build-types": "NODE_OPTIONS='--max-old-space-size=4096' node src/scripts/compileTypes.js",
		"build": "NODE_OPTIONS='--max-old-space-size=4096' npm run typesafe-i18n && npm run build-types && tsc --project tsconfig.json",
		"commands:view": "npm run build && node --enable-source-maps dist/src/start-bot.js commands view",
		"commands:register": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && node --enable-source-maps dist/src/start-bot.js commands register",
		"commands:rename": "npm run build && node --enable-source-maps dist/src/start-bot.js commands rename",
		"commands:delete": "npm run build && node --enable-source-maps dist/src/start-bot.js commands delete",
		"commands:clear": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && node --enable-source-maps dist/src/start-bot.js commands clear",
		"start": "npm run start:bot",
		"start:bot": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && node --enable-source-maps dist/src/start-bot.js",
		"start:manager": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && node --enable-source-maps dist/src/start-manager.js",
		"start:prod": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && npm run commands:clear-prod && npm run commands:register-prod && npm run start:manager-prod",
		"start:pm2": "npm run build && npm run pm2:start",
		"pm2:start": "pm2 start process.json",
		"pm2:stop": "pm2 stop process.json",
		"pm2:delete": "pm2 delete process.json",
		"typesafe-i18n": "NODE_OPTIONS='--max-old-space-size=4096' typesafe-i18n --no-watch",
		"typesafe-i18n:watch": "typesafe-i18n",
		"knex": "NODE_OPTIONS='--max-old-space-size=4096' NODE_OPTIONS='--loader ts-node/esm' knex",
		"migrate": "NODE_OPTIONS='--max-old-space-size=4096' NODE_OPTIONS='--loader ts-node/esm' knex migrate:latest",
		"migrate:test": "NODE_ENV=TEST NODE_OPTIONS='--loader ts-node/esm' knex migrate:latest",
		"test": "vitest",
		"replace-i18n-types": "cp ./src/i18n/i18n-types.prod.ts ./src/i18n/i18n-types.ts",
		"build-prod": "NODE_OPTIONS='--max-old-space-size=4096' npm run replace-i18n-types && npm run build-types && tsc --project tsconfig.json",
		"commands:clear-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/src/start-bot.js commands clear ",
		"commands:register-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/src/start-bot.js commands register",
		"start:bot-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/src/start-bot.js",
		"start:manager-prod": "NODE_OPTIONS='--max-old-space-size=4096' node dist/src/start-manager.js",
		"import:pf2etools": "NODE_OPTIONS='--max-old-space-size=4096' npm run build && node dist/src/services/pf2etools/pf2eTools-import.js"
	},
	"dependencies": {
		"@faker-js/faker": "^8.0.2",
		"better-sqlite3": "^8.6.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.0",
		"csv": "^6.3.4",
		"date-fns": "^2.30.0",
		"dice-roller-parser": "^0.1.8",
		"dice-typescript": "^1.6.1",
		"discord.js": "^14.13.0",
		"discord.js-rate-limiter": "1.3.2",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.6",
		"drizzle-zod": "^0.5.1",
		"express": "4.18.2",
		"express-promise-router": "4.1.1",
		"filesize": "10.0.12",
		"filtrex": "^3.0.0",
		"fishery": "^2.2.2",
		"form-data": "^4.0.0",
		"json-schema": "^0.4.0",
		"knex": "^2.5.1",
		"kysely": "^0.26.3",
		"linguini": "1.3.1",
		"lodash": "^4.17.21",
		"luxon": "3.4.3",
		"node-fetch": "3.3.2",
		"node-schedule": "2.1.1",
		"objection": "^3.1.1",
		"pg": "^8.11.3",
		"pino": "8.15.1",
		"pino-pretty": "10.2.0",
		"pm2": "^5.3.0",
		"reflect-metadata": "^0.1.13",
		"remove-markdown": "0.5.0",
		"table": "^6.8.1",
		"ts-json-schema-generator": "^1.3.0",
		"ts-mixer": "^6.0.3",
		"type-fest": "^4.3.1",
		"typesafe-i18n": "^5.26.2",
		"xxhashjs": "^0.2.2",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.5",
		"@types/express": "4.17.17",
		"@types/luxon": "3.3.2",
		"@types/node": "^20.6.0",
		"@types/node-schedule": "2.1.0",
		"@types/pg": "^8.10.2",
		"@types/remove-markdown": "0.3.1",
		"@types/xxhashjs": "^0.2.2",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.49.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-unicorn": "^48.0.1",
		"glob": "^10.3.4",
		"json-schema-to-typescript": "^13.1.1",
		"json-schema-to-zod": "^1.1.1",
		"kanel": "^3.5.5",
		"kanel-kysely": "^0.2.1",
		"kanel-zod": "^1.2.2",
		"madge": "^6.1.0",
		"minimatch": "^9.0.3",
		"prettier": "^3.0.3",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.9.1",
		"ts-to-zod": "^3.1.3",
		"typescript": "^5.2",
		"vitest": "^0.34.4"
	},
	"madge": {
		"tsConfig": "./tsconfig.json",
		"detectiveOptions": {
			"ts": {
				"skipTypeImports": true
			}
		}
	}
}
